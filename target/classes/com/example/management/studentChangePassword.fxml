<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.management.controller.StudentChangePasswordController">
   <children>
      <Label layoutX="250.0" layoutY="18.0" text="change password" />
      <TextField fx:id="currentPassword" layoutX="200.0" layoutY="93.0" prefHeight="25.0" prefWidth="211.0" promptText="current password" />
      <TextField fx:id="newPassword" layoutX="200.0" layoutY="149.0" prefHeight="25.0" prefWidth="211.0" promptText="new password" />
      <TextField fx:id="confrimNewPassword" layoutX="200.0" layoutY="205.0" prefHeight="25.0" prefWidth="211.0" promptText="confrim new password" />
      <Button fx:id="btnSubmit" layoutX="236.0" layoutY="269.0" mnemonicParsing="false" onAction="#btnSubmitAction" prefHeight="25.0" prefWidth="123.0" text="submit" />
      <Label fx:id="txtError" contentDisplay="CENTER" layoutX="15.0" layoutY="333.0" prefHeight="17.0" prefWidth="555.0" textAlignment="CENTER" />
      <Button fx:id="btnBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnBackAction" text="back" />
   </children>
</AnchorPane>
