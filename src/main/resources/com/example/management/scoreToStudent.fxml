      <Label fx:id="txtError" alignment="CENTER" contentDisplay="CENTER" layoutX="6.0" layoutY="207.0" prefHeight="17.0" prefWidth="414.0" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS" />
